[global]
kb_base=1000
verify=crc32c
#verify_only
do_verify=0

[write_one_sequential]
filesize=1GiB
readwrite=write
numjobs=1
stonewall

[write_one_random]
filesize=1GiB
readwrite=randwrite
numjobs=1
stonewall

[write_many_sequential]
filesize=1MiB
readwrite=write
numjobs=1024
stonewall

[write_many_random]
filesize=1MiB
readwrite=randwrite
numjobs=1024
stonewall
